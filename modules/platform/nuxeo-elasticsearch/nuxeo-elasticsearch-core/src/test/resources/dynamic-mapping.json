{
  "dynamic_templates": [
    {
      "dynamic_string": {
        "match": "*_string",
        "mapping": {
          "type": "text"
        }
      }
    },
    {
      "dynamic_int": {
        "match": "*_int",
        "mapping": {
          "type": "integer"
        }
      }
    },
    {
      "dynamic_date": {
        "match": "*_date",
        "mapping": {
          "type": "date"
        }
      }
    }
  ]
}
